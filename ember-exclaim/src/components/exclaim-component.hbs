{{#component
  @wrapper
  componentSpec=@componentSpec
  env=this.effectiveEnv
  config=this.resolvedConfig
}}
  {{#component
    @componentSpec.component
    config=this.resolvedConfig
    env=this.effectiveEnv
    as |componentSpec overrideEnv|
  }}
    {{~exclaim-component
      componentSpec=componentSpec
      env=this.effectiveEnv
      overrideEnv=overrideEnv
      wrapper=@wrapper
    ~}}
  {{/component}}
{{/component}}