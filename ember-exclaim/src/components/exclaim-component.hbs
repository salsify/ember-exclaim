<@wrapper
  @componentSpec={{@componentSpec}}
  @env={{this.componentData.env}}
  @config={{this.componentData.config}}
>
  {{~null~}}
  <@componentSpec.component
    @config={{this.componentData.config}}
    @env={{this.componentData.env}}
    as |componentSpec additionalEnvData|
  >
    {{~null~}}
    <ExclaimComponent
      @componentSpec={{componentSpec}}
      @env={{this.componentData.env}}
      @additionalEnvData={{additionalEnvData}}
      @wrapper={{@wrapper}}
    />
    {{~null~}}
  </@componentSpec.component>
  {{~null~}}
</@wrapper>